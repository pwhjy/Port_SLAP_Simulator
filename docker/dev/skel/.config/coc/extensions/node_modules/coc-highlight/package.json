{
  "name": "coc-highlight",
  "version": "1.0.5",
  "description": "Highlight extension for coc.nvim",
  "main": "lib/index.js",
  "publisher": "chemzqm",
  "keywords": [
    "coc.nvim",
    "highlight",
    "colors"
  ],
  "engines": {
    "coc": "^0.0.32"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc -p tsconfig.json",
    "prepare": "yarn clean && yarn build"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Highlight",
      "properties": {
        "highlight.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ]
        },
        "highlight.execArgv": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "highlight.disableLanguages": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "list of filetypes to ignore."
        },
        "highlight.document.enable": {
          "type": "boolean",
          "default": true,
          "description": "Set to false disable document highlight of current symbol."
        },
        "highlight.colors.enable": {
          "type": "boolean",
          "default": true,
          "description": "set to false to disable color highlight."
        }
      }
    }
  },
  "author": "chemzqm@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.17",
    "@types/color": "^3.0.0",
    "@types/color-name": "^1.1.0",
    "@types/debounce": "^3.0.0",
    "@types/node": "^10.12.9",
    "coc.nvim": "^0.0.32",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "color": "^3.1.0",
    "color-name": "^1.1.4",
    "debounce": "^1.2.0",
    "tslib": "^1.9.3",
    "vscode-languageserver": "^5.1.0",
    "vscode-languageserver-protocol": "^3.13.0",
    "vscode-uri": "^1.0.6"
  }
}
